{
    "name": "andrey-helldar/cashier-tinkoff-auth",
    "description": "Tinkoff API Authorization Driver",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "andrey-helldar",
        "bank",
        "billing",
        "cashier",
        "tinkoff",
        "auth"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/cashier-tinkoff-auth/issues",
        "source": "https://github.com/andrey-helldar/cashier-tinkoff-auth"
    },
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "andrey-helldar/contracts": "^1.4.5",
        "andrey-helldar/support": "^4.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\CashierDriver\\Tinkoff\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
